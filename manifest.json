{
    "description": "To track amount spent a tab",
    "manifest_version": 2,
    "name": "ChromyTrack",
    "version": "0.7.1",
  
    "permissions": ["tabs", "storage", "cookies"],
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {}
}